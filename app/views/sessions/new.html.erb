<h1>Sessions#new</h1>

<%= form_tag sessions_path do %>
<div class="field">
  <%= label_tag :email %>
  <%= text_field_tag :email %>
</div>

<div class="field">
  <%= label_tag :password %>
  <%= text_field_tag :password %>
</div>

<div class="actions">
    <%= submit_tag "Sign In" %>
  </div>
  <% end %>
  
<p>Find me in app/views/sessions/new.html.erb</p>
