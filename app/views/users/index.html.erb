<!DOCTYPE html>
<html>
  <head>
    <meta charset="utf-8">
    <title></title>
  </head>
  <body>
    <h1>Welcome</h1>

    <table>
      <thead>
        <tr>
          <th>Email</th>
          <th>Password digest</th>
          <th colspan="3"></th>
        </tr>
      </thead>

      <tbody>
        <% @users.each do |user| %>
          <tr>
            <td><%= user.email %></td>
            <td><%= user.password_digest %></td>

            <td><%= link_to 'Show', user %></td>

          </tr>
        <% end %>
      </tbody>
    </table>

  </body>
</html>
